<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
     
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="35dp">
        
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            >
            
	        <RelativeLayout
	            android:layout_width="135dp"
	            android:layout_height="match_parent"
	            android:layout_alignParentLeft="true"
	            >
		        <View 
		            android:background="#ddd"
					android:layout_height="match_parent"
					android:layout_alignParentRight="true"
					android:layout_width="1dp"/>

		        <TextView
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:layout_alignParentLeft="true"
		            android:layout_alignParentTop="true"
		            android:gravity="right|center_vertical"
		            android:paddingRight="10dp"
		            android:text="当月排名"
		            android:textSize="@dimen/fonts_16" />
	        
	        </RelativeLayout>
	        
	        
	         <TextView
	            android:id="@+id/rowMonth"
	            android:layout_width="match_parent"
	            android:layout_height="match_parent"
	            android:paddingLeft="10dp"
	            android:gravity="center"
	            android:textSize="@dimen/fonts_16"
	            android:background="#fff"
	            />
            
        </LinearLayout>
        
        
        <View 
            android:background="#ddd"
			android:layout_height="match_parent"
			android:layout_alignParentRight="true"
			android:layout_width="1dp"/>
        
         <View 
            android:background="#ddd"
			android:layout_width="match_parent"
			android:layout_alignParentTop="true"
			android:layout_height="1dp"/>
        
         <View 
            android:background="#ddd"
			android:layout_height="match_parent"
			android:layout_alignParentLeft="true"
			android:layout_width="1dp"/>
    </RelativeLayout>
    
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="35dp">
        
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            >
            
	        <RelativeLayout
	            android:layout_width="135dp"
	            android:layout_height="match_parent"
	            android:layout_alignParentLeft="true"
	            >
	            
		        <TextView
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:gravity="right|center_vertical"
		            android:paddingRight="10dp"
		            android:textSize="@dimen/fonts_16"
		            android:text="当月销售成金额(万元)"
		            />
		        <View 
		            android:background="#ddd"
					android:layout_height="match_parent"
					android:layout_alignParentRight="true"
					android:layout_width="1dp"/>
	        
	        </RelativeLayout>
	        
	        
	         <TextView
	            android:id="@+id/total"
	            android:layout_width="match_parent"
	            android:layout_height="match_parent"
	            android:paddingLeft="10dp"
	            android:gravity="center"
	            android:textSize="@dimen/fonts_16"
	            android:background="#fff" />
            
        </LinearLayout>
        
        
        <View 
            android:background="#ddd"
			android:layout_height="match_parent"
			android:layout_alignParentRight="true"
			android:layout_width="1dp"/>
        
         <View 
            android:background="#ddd"
			android:layout_width="match_parent"
			android:layout_alignParentTop="true"
			android:layout_height="1dp"/>
        
         <View 
            android:background="#ddd"
			android:layout_height="match_parent"
			android:layout_alignParentLeft="true"
			android:layout_width="1dp"/>
        
    </RelativeLayout>
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="35dp">
        
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            >
            
	        <RelativeLayout
	            android:layout_width="135dp"
	            android:layout_height="match_parent"
	            android:layout_alignParentLeft="true"
	            >
	            
		        <TextView
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:gravity="right|center_vertical"
		            android:textSize="@dimen/fonts_16"
		            android:paddingRight="10dp"
		            android:text="项目名称"
		            />
		        <View 
		            android:background="#ddd"
					android:layout_height="match_parent"
					android:layout_alignParentRight="true"
					android:layout_width="1dp"/>
	        
	        </RelativeLayout>
	        
	        
	         <TextView
	            android:id="@+id/name"
	            android:layout_width="match_parent"
	            android:layout_height="match_parent"
	            android:paddingLeft="10dp"
	            android:gravity="center"
	            android:textSize="@dimen/fonts_16"
	            android:background="#fff"
	            />
            
        </LinearLayout>
        
        
        <View 
            android:background="#ddd"
			android:layout_height="match_parent"
			android:layout_alignParentRight="true"
			android:layout_width="1dp"/>
        
         <View 
            android:background="#ddd"
			android:layout_width="match_parent"
			android:layout_alignParentTop="true"
			android:layout_height="1dp"/>
        
         <View 
            android:background="#ddd"
			android:layout_height="match_parent"
			android:layout_alignParentLeft="true"
			android:layout_width="1dp"/>

    </RelativeLayout>
    
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="35dp">
        
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            >
            
	        <RelativeLayout
	            android:layout_width="135dp"
	            android:layout_height="match_parent"
	            android:layout_alignParentLeft="true"
	            >
	            
		        <TextView
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:gravity="right|center_vertical"
		            android:textSize="@dimen/fonts_16"
		            android:paddingRight="10dp"
		            android:text="均价(元/m²)"
		            />
		        <View 
		            android:background="#ddd"
					android:layout_height="match_parent"
					android:layout_alignParentRight="true"
					android:layout_width="1dp"/>
	        
	        </RelativeLayout>
	        
	        
	         <TextView
	            android:id="@+id/jj"
	            android:layout_width="match_parent"
	            android:layout_height="match_parent"
	            android:textSize="@dimen/fonts_16"
	            android:background="#fff"
	            android:paddingLeft="10dp"
	            android:gravity="center"
	            />
            
        </LinearLayout>
        
        
        <View 
            android:background="#ddd"
			android:layout_height="match_parent"
			android:layout_alignParentRight="true"
			android:layout_width="1dp"/>
        
         <View 
            android:background="#ddd"
			android:layout_width="match_parent"
			android:layout_alignParentTop="true"
			android:layout_height="1dp"/>
        
         <View 
            android:background="#ddd"
			android:layout_height="match_parent"
			android:layout_alignParentLeft="true"
			android:layout_width="1dp"/>
        
    </RelativeLayout>
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="35dp">
        
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            >
            
	        <RelativeLayout
	            android:layout_width="135dp"
	            android:layout_height="match_parent"
	            android:layout_alignParentLeft="true"
	            >
	            
		        <TextView
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:gravity="right|center_vertical"
		            android:textSize="@dimen/fonts_16"
		            android:paddingRight="10dp"
		            android:text="全年排名"
		            />
		        <View 
		            android:background="#ddd"
					android:layout_height="match_parent"
					android:layout_alignParentRight="true"
					android:layout_width="1dp"/>
	        
	        </RelativeLayout>
	        
	        
	         <TextView
	            android:id="@+id/rowYear"
	            android:layout_width="match_parent"
	            android:layout_height="match_parent"
	            android:textSize="@dimen/fonts_16"
	            android:background="#fff"
	            android:paddingLeft="10dp"
	            android:gravity="center"
	            />
            
        </LinearLayout>
        
        
        <View 
            android:background="#ddd"
			android:layout_height="match_parent"
			android:layout_alignParentRight="true"
			android:layout_width="1dp"/>
        
         <View 
            android:background="#ddd"
			android:layout_width="match_parent"
			android:layout_alignParentTop="true"
			android:layout_height="1dp"/>
        
         <View 
            android:background="#ddd"
			android:layout_height="match_parent"
			android:layout_alignParentLeft="true"
			android:layout_width="1dp"/>
        
    </RelativeLayout>
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="35dp">
        
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            >
            
	        <RelativeLayout
	            android:layout_width="135dp"
	            android:layout_height="match_parent"
	            android:layout_alignParentLeft="true"
	            >
	            
		        <TextView
		            android:layout_width="match_parent"
		            android:layout_height="match_parent"
		            android:gravity="right|center_vertical"
		            android:textSize="@dimen/fonts_16"
		            android:paddingRight="10dp"
		            android:text="地理位置"
		            />
		        <View 
		            android:background="#ddd"
					android:layout_height="match_parent"
					android:layout_alignParentRight="true"
					android:layout_width="1dp"/>
	        
	        </RelativeLayout>
	        
	        
	         <TextView
	            android:id="@+id/place"
	            android:layout_width="match_parent"
	            android:layout_height="match_parent"
	            android:textSize="@dimen/fonts_16"
	            android:background="#fff"
	            android:paddingLeft="10dp"
	            android:gravity="center"
	            />
            
        </LinearLayout>
        
        
        <View 
            android:background="#ddd"
			android:layout_height="match_parent"
			android:layout_alignParentRight="true"
			android:layout_width="1dp"/>
        
         <View 
            android:background="#ddd"
			android:layout_width="match_parent"
			android:layout_alignParentTop="true"
			android:layout_height="1dp"/>
        
         <View 
            android:background="#ddd"
			android:layout_height="match_parent"
			android:layout_alignParentLeft="true"
			android:layout_width="1dp"/>
        
    </RelativeLayout>
    
	<View 
        android:background="#ddd"
		android:layout_height="1dp"
		android:layout_width="match_parent"/>

</LinearLayout>
